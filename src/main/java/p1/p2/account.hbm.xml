<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
  <hibernate-mapping>
  
  <class name="p1.p2.App">
   <id name="citizenId">
   <generator class="increment"></generator>
   </id>
   
   <property name="accountType" column="account_Type"/>
   <property name="accountHolderName" column="account_Holder_Name"/>
   <property name="bankName" column="bank_Name"/>
   <property name="branchName" column="branch_Name"/>
   <property name="branchCode" column="branch_Code"/>
   
  </class>
  
  </hibernate-mapping>